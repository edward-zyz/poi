{
  "name": "@poi/backend",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --exec node --loader ts-node/esm src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "NODE_ENV=test node --loader ts-node/esm --test test/*.test.ts",
    "gaode:test": "node --loader ts-node/esm src/scripts/gaodeSample.ts",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "zod": "^3.22.2",
    "better-sqlite3": "^9.0.0",
    "pino": "^8.15.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "nodemon": "^3.0.2"
  }
}
